{
  "name": "gh_cap_fiori_exercise",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "dependencies": {
    "@cap-js/audit-logging": "^1.1.1",
    "@sap/cds": "^9",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^2",
    "@sap/cds-dk": "^9",
    "cds-plugin-ui5": "^0.13.0",
    "cds-swagger-ui-express": "^0.10.0",
    "eslint": "^9.38.0",
    "mbt": "^1.2.29",
    "rimraf": "^5.0.5"
  },
  "scripts": {
    "start": "cds-serve",
    "kill_port": "kill -9 $(lsof -t -i:4004)",
    "undeploy": "cf undeploy gh_cap_fiori_exercise --delete-services --delete-service-keys --delete-service-brokers",
    "build": "rimraf resources mta_archives && mbt build --mtar archive",
    "deploy": "cf deploy mta_archives/archive.mtar --retries 1",
    "watch-frontend_projects": "cds watch --open innova.tech.front.frontendprojects/index.html?sap-ui-xx-viewCache=false --livereload false",
    "watch-frontend_employees": "cds watch --open innova.tech.front.frontendemployees/index.html?sap-ui-xx-viewCache=false --livereload false",
    "watch-frontend_workentries": "cds watch --open innova.tech.front.frontendworkentries/index.html?sap-ui-xx-viewCache=false --livereload false"
  },
  "cds": {
    "requires": {
      "db": {
        "kind": "sqlite",
        "credentials": {
          "url": "sqlite.db"
        }
      }
    }
  },
  "private": true,
  "workspaces": [
    "app/*"
  ],
  "sapux": [
    "app/frontend_projects",
    "app/frontend_employees",
    "app/frontend_workentries"
  ]
}
